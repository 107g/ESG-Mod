<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation="../Schemas/QuestDefinition.xsd">
  <!-- All quests in this file remove a specific find of faction trait after their effect is applied.
       This only applies to things that have a one-time effect, like luxury spawning, or AI personality -->

  <!-- AI Personality -->
  <QuestDefinition Name="RemovePersonalityTraitsAfterAssignment" Category="Start" SubCategory="Regular" TriggeringProbability="1" MinimumTurn="0">
    <Tags>Hidden,BeginTurn</Tags>
    <QuestContextSolo/>
    <RepetitionRules NumberOfOccurrencesPerEmpire="1"/>
    <Vars>
      <Var VarName="$CurrentEmpire">
        <From Source="$Empire"/>
      </Var>
      <Var VarName="$Trait1"  StringValue="PersonalityCanGetAny"/>
      <Var VarName="$Trait2"  StringValue="PersonalityCanGetMilitarist"/>
      <Var VarName="$Trait3"  StringValue="PersonalityCanGetPacifist"/>
      <Var VarName="$Trait4"  StringValue="PersonalityCanGetParagon"/>
      <Var VarName="$Trait5"  StringValue="PersonalityCanGetRenegade"/>
      <Var VarName="$Trait6"  StringValue="PersonalityCanGetMercantile"/>
      <Var VarName="$Trait7"  StringValue="PersonalityCanGetIsolationist"/>
      <Var VarName="$Trait8"  StringValue="PersonalityCanGetCruel"/>
      <Var VarName="$Trait9"  StringValue="PersonalityCanGetRestless"/>
      <Var VarName="$Trait10" StringValue="PersonalityCanGetInnovative"/>
      <Var VarName="$Trait11" StringValue="PersonalityCanGetBenevolent"/>
      <Var VarName="$Trait12" StringValue="PersonalityCanGetGreedy"/>
    </Vars>
    <Steps>
      <Step Name="Step1">
        <ObjectiveSet>
          <Objective Name="Add_Trait">
            <Sequence>
              <Action_ApplyDescriptor LogAction="false" Remove="true">
                <Input_DescriptorName VarName="$Trait1"/>
                <Input_Targets VarName="$CurrentEmpire"/>
              </Action_ApplyDescriptor>

              <Action_ApplyDescriptor LogAction="false" Remove="true">
                <Input_DescriptorName VarName="$Trait2"/>
                <Input_Targets VarName="$CurrentEmpire"/>
              </Action_ApplyDescriptor>

              <Action_ApplyDescriptor LogAction="false" Remove="true">
                <Input_DescriptorName VarName="$Trait3"/>
                <Input_Targets VarName="$CurrentEmpire"/>
              </Action_ApplyDescriptor>

              <Action_ApplyDescriptor LogAction="false" Remove="true">
                <Input_DescriptorName VarName="$Trait4"/>
                <Input_Targets VarName="$CurrentEmpire"/>
              </Action_ApplyDescriptor>

              <Action_ApplyDescriptor LogAction="false" Remove="true">
                <Input_DescriptorName VarName="$Trait5"/>
                <Input_Targets VarName="$CurrentEmpire"/>
              </Action_ApplyDescriptor>

              <Action_ApplyDescriptor LogAction="false" Remove="true">
                <Input_DescriptorName VarName="$Trait6"/>
                <Input_Targets VarName="$CurrentEmpire"/>
              </Action_ApplyDescriptor>

              <Action_ApplyDescriptor LogAction="false" Remove="true">
                <Input_DescriptorName VarName="$Trait7"/>
                <Input_Targets VarName="$CurrentEmpire"/>
              </Action_ApplyDescriptor>

              <Action_ApplyDescriptor LogAction="false" Remove="true">
                <Input_DescriptorName VarName="$Trait8"/>
                <Input_Targets VarName="$CurrentEmpire"/>
              </Action_ApplyDescriptor>

              <Action_ApplyDescriptor LogAction="false" Remove="true">
                <Input_DescriptorName VarName="$Trait9"/>
                <Input_Targets VarName="$CurrentEmpire"/>
              </Action_ApplyDescriptor>

              <Action_ApplyDescriptor LogAction="false" Remove="true">
                <Input_DescriptorName VarName="$Trait10"/>
                <Input_Targets VarName="$CurrentEmpire"/>
              </Action_ApplyDescriptor>

              <Action_ApplyDescriptor LogAction="false" Remove="true">
                <Input_DescriptorName VarName="$Trait11"/>
                <Input_Targets VarName="$CurrentEmpire"/>
              </Action_ApplyDescriptor>

              <Action_ApplyDescriptor LogAction="false" Remove="true">
                <Input_DescriptorName VarName="$Trait12"/>
                <Input_Targets VarName="$CurrentEmpire"/>
              </Action_ApplyDescriptor>
            </Sequence>
          </Objective>
        </ObjectiveSet>
      </Step>
    </Steps>
  </QuestDefinition>
