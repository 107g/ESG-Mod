  <!-- Empire 0 -->
  <QuestDefinition Name="QuestDecidePreTeamEmpire0" IsInstant="true" Category="GameSetting" SubCategory="PreTeams" TriggeringProbability="1" MinimumTurn="0">
    <Tags>Hidden,BeginTurn</Tags>
    <QuestContextSolo/>
    <RepetitionRules NumberOfOccurrencesPerEmpire="1"/>
    <Vars>
      <Var VarName="$CurrentEmpire">
        <From Source="$Empire"/>
      </Var>

      <Var VarName="$Team" StringValue="FactionTraitIsTeam1"/>
    </Vars>
    <Prerequisites Target="$(Empire)">
      <PathPrerequisite Flags="Prerequisite">ClassEmpire,FactionTraitIsEmpire0</PathPrerequisite>
      <GameSettingPrerequisite Inverted="true">PreTeams,0</GameSettingPrerequisite>
      <GameSettingPrerequisite Inverted="true">PreTeams,-1</GameSettingPrerequisite>
    </Prerequisites>
    <Steps>
      <Step Name="Step1">
        <ObjectiveSet>
          <Objective Name="Add_Team">
            <Sequence>
              <Action_ApplyDescriptor LogAction="false">
                <Input_DescriptorName VarName="$Team"/>
                <Input_Targets VarName="$CurrentEmpire"/>
              </Action_ApplyDescriptor>
            </Sequence>
          </Objective>
        </ObjectiveSet>
      </Step>
    </Steps>
  </QuestDefinition>

