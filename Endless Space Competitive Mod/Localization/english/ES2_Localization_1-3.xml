  <!-- ESG Settings -->
  <LocalizationPair Name="%AdvancedSettingsESGTitle">ESG Mod Settings</LocalizationPair>
  
  <!-- Balanced Start -->
  <LocalizationPair Name="%SettingBalancedStartsTitle">Balance Player Starts</LocalizationPair>
  <LocalizationPair Name="%SettingBalancedStartsDescription">Enabling this will guarantee that factions spawn with planets specifically chosen for them.</LocalizationPair>
  <LocalizationPair Name="%SettingBalancedStartsTrueTitle">True</LocalizationPair>
  <LocalizationPair Name="%SettingBalancedStartsTrueDescription">Factions will spawn with specifically chosen home systems.</LocalizationPair>
  <LocalizationPair Name="%SettingBalancedStartsFalseTitle">False</LocalizationPair>
  <LocalizationPair Name="%SettingBalancedStartsFalseDescription">Factions will spawn with random star systems.</LocalizationPair>

  <!-- Luxury Rework -->
  <LocalizationPair Name="%SettingLuxuryReworkTitle">Enable Luxury Rework</LocalizationPair>
  <LocalizationPair Name="%SettingLuxuryReworkDescription">Enabling this make luxuries have their revised ESG effects in system development.</LocalizationPair>
  <LocalizationPair Name="%SettingLuxuryReworkTrueTitle">True</LocalizationPair>
  <LocalizationPair Name="%SettingLuxuryReworkTrueDescription">Luxuries will have their revised ESG effects in system development.</LocalizationPair>
  <LocalizationPair Name="%SettingLuxuryReworkFalseTitle">False</LocalizationPair>
  <LocalizationPair Name="%SettingLuxuryReworkFalseDescription">Luxuries will have their vanilla effects in system development.</LocalizationPair>

  <!-- Scale Supremacy -->
  <LocalizationPair Name="%SettingScaleSupremacyVictoryTitle">Scale Supremacy Victory</LocalizationPair>
  <LocalizationPair Name="%SettingScaleSupremacyVictoryDescription">Enabling this give defensive bonuses to players not in an alliance, so that it scales like all other victory types.</LocalizationPair>
  <LocalizationPair Name="%SettingScaleSupremacyVictoryTrueTitle">True</LocalizationPair>
  <LocalizationPair Name="%SettingScaleSupremacyVictoryTrueDescription">Home systems will gain defensive bonuses based off of number of players, and lose it per alliance member.</LocalizationPair>
  <LocalizationPair Name="%SettingScaleSupremacyVictoryFalseTitle">False</LocalizationPair>
  <LocalizationPair Name="%SettingScaleSupremacyVictoryFalseDescription">Supremacy Victory will work like it does in vanilla.</LocalizationPair>

  <!-- Tradeable Curiosity Techs-->
  <LocalizationPair Name="%SettingSettingTradeableCuriosityTechsTitle">Tradeable Curiosity Techs</LocalizationPair>
  <LocalizationPair Name="%SettingSettingTradeableCuriosityTechsDescription">Enabling this will allow you to trade rare modules and system improvements between players..</LocalizationPair>
  <LocalizationPair Name="%SettingSettingTradeableCuriosityTechsTrueTitle">True</LocalizationPair>
  <LocalizationPair Name="%SettingSettingTradeableCuriosityTechsTrueDescription">Curiosity technologies are tradeable.</LocalizationPair>
  <LocalizationPair Name="%SettingSettingTradeableCuriosityTechsFalseTitle">False</LocalizationPair>
  <LocalizationPair Name="%SettingSettingTradeableCuriosityTechsFalseDescription">Curiosity technologies are not tradeable.</LocalizationPair>

  <!-- Terraform Size -->
  <LocalizationPair Name="%SettingTerraformSizeTitle">Enable Size Terraformation</LocalizationPair>
  <LocalizationPair Name="%SettingTerraformSizeDescription">Enabling this will allow you to terraform planets to increase size. Disabling this will make it just like vanilla. Enabling free terraformation with this enabled will allow you to terraform planet sizes down.</LocalizationPair>
  <LocalizationPair Name="%SettingTerraformSizeTrueTitle">True</LocalizationPair>
  <LocalizationPair Name="%SettingTerraformSizeTrueDescription">You will be able to terraform planet sizes.</LocalizationPair>
  <LocalizationPair Name="%SettingTerraformSizeFalseTitle">False</LocalizationPair>
  <LocalizationPair Name="%SettingTerraformSizeFalseDescription">You will not be able to terraform planet sizes.</LocalizationPair>

  <!-- Terraform Jovian -->
  <LocalizationPair Name="%SettingTerraformJovianTitle">Enable Jovian Terraformation</LocalizationPair>
  <LocalizationPair Name="%SettingTerraformJovianDescription">Enabling this will allow you to terraform gas planets. Disabling this will make you unable to terraform gas planets. Enabling free terraformation with this enabled will allow you to terraform between telluric and jovian planets.</LocalizationPair>
  <LocalizationPair Name="%SettingTerraformJovianTrueTitle">True</LocalizationPair>
  <LocalizationPair Name="%SettingTerraformJovianTrueDescription">You will be able to terraform gas planets.</LocalizationPair>
  <LocalizationPair Name="%SettingTerraformJovianFalseTitle">False</LocalizationPair>
  <LocalizationPair Name="%SettingTerraformJovianFalseDescription">You will not be able to terraform gas planets.</LocalizationPair>

