<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation="../Schemas/DiplomaticPressureEffectDefinition.xsd">
  <!-- Praise -->
  <DiplomaticPressureEffectDefinition_Turns Name="PressureReductionFromDiplomacy" Duration="10">
    <SimulationDescriptorReferenceOnInitiator Name="PressureReductionFromDiplomacyInitiator"/>
    <SimulationDescriptorReferenceOnInitiator Name="GeneratePressureTermActivate"/>
    <SimulationDescriptorReferenceOnTarget Name="PressureReductionFromDiplomacyTarget"/>
  </DiplomaticPressureEffectDefinition_Turns>

  <!-- Warning -->
  <DiplomaticPressureEffectDefinition_Turns Name="DiplomaticWarning" Duration="10">
    <SimulationDescriptorReferenceOnInitiator  Name="DiplomaticWarningInitiator"/>
  </DiplomaticPressureEffectDefinition_Turns>

  <DiplomaticPressureEffectDefinition_Turns Name="DiplomaticWarningMilitarists" Duration="5">
    <SimulationDescriptorReferenceOnInitiator  Name="DiplomaticWarningInitiator"/>
  </DiplomaticPressureEffectDefinition_Turns>

  <!-- Prepare for War -->
  <DiplomaticPressureEffectDefinition_Turns Name="PrepareForWar" Duration="15">
    <SimulationDescriptorReferenceOnInitiator  Name="PrepareForWarInitiator"/>
  </DiplomaticPressureEffectDefinition_Turns>

  <!-- Forced Agreements -->
  <DiplomaticPressureEffectDefinition_Turns Name="TradeAgreementForcedPressureEffect" Duration="9999">
    <SimulationDescriptorReferenceOnInitiator Name="TradeAgreementForcedPressureInitiator"/>
    <SimulationDescriptorReferenceOnTarget    Name="TradeAgreementForcedPressureTarget"/>
  </DiplomaticPressureEffectDefinition_Turns>

  <DiplomaticPressureEffectDefinition_Turns Name="ScienceAgreementForcedPressureEffect" Duration="9999">
    <SimulationDescriptorReferenceOnInitiator Name="ScienceAgreementForcedPressureInitiator"/>
    <SimulationDescriptorReferenceOnTarget    Name="ScienceAgreementForcedPressureTarget"/>
  </DiplomaticPressureEffectDefinition_Turns>
</Datatable>